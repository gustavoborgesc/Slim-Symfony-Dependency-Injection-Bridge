{
    "name": "flexsounds/slim-symfony-di-container",
    "description": "The bridge to use the Symfony Dependency Injection Container to use in Slim Framework 3",
    "type": "library",
    "require": {
        "symfony/dependency-injection": "^3.0",
        "symfony/config": "^3.0",
        "symfony/yaml": "^3.0"
    },
    "require-dev": {
        "slim/slim": "^3.1",
    },
    "suggest": {
        "slim/slim": "To use this bridge, you have to install Slim Framework 3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Leroy Baeyens",
            "email": "info@leroybaeyens.nl"
        }
    ],
    "autoload": {
        "psr-4": ["Flexsounds\\Component\\SymfonyContainerSlimBridge\\", "src/"]
    },
    "minimum-stability": "stable"
}
